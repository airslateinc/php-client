{
    "name": "pdffiller/airslate-php-client",
    "description": "AirSlate client library.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AirSlate\\ApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/unit"
        }
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "6.2.*|6.3.*",
        "doctrine/annotations": "~1.6",
        "jms/serializer": "~1.11",
        "illuminate/config": "^5.6",
        "illuminate/container": "^5.6",
        "symfony/http-foundation": "~4.0",
        "symfony/http-kernel": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    }
}
