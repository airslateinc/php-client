{
    "name": "pdffiller/airslate-php-client",
    "description": "AirSlate client library.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AirSlate\\ApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AirSlate\\ApiClientTests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "6.2.*|6.3.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^7.3"
    }
}
